{
  "name": "@ivoryio/ivory-cli",
  "version": "1.1.0-alpha.0",
  "description": "Ivory CLI",
  "main": "index.js",
  "bin": {
    "ivory": "bin/ivory-cli.js"
  },
  "scripts": {
    "publish:npm": "yarn test && npm publish --access public --tag alpha",
