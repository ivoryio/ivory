    "test:watch": "mocha --require ts-node/register --watch-files src --watch \"src/tests/**/*.test.ts\"",
    "build": "rm -r ./lib && yarn tsc",
    "lint": "eslint './src/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoryio/ivory.git",
