  "license": "GPL-3.0-or-later",
  "private": false,
  "bugs": {
    "url": "https://github.com/ivoryio/ivory/issues"
  },
  "homepage": "https://github.com/ivoryio/ivory#readme",
  "files": [
    "/lib",
    "/bin"
  ],
  "dependencies": {
    "aws-sdk": "^2.683.0",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "fs-extra": "^9.0.1",
    "ini": "^1.3.5",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "os": "^0.1.1",
    "shelljs": "^0.8.4",
    "typescript": "^3.9.2"
  },
