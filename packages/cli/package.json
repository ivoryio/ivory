    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2"
  },
  "peerDependencies": {
    "@aws-amplify/cli": "^4.20.0"
